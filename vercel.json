
{
  "version": 2,
  "builds": [
    {
      "src": "src/server/start-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/start-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/server/start-server.js"
    }
  ],
  "installCommand": "bun install"
}
