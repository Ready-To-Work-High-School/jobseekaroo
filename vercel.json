
{
  "version": 2,
  "builds": [
    {
      "src": "src/server/start-server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/start-server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "src/server/start-server.cjs"
    }
  ],
  "installCommand": "bun install",
  "buildCommand": "bun run build"
}
